
def factorial(integer a) : integer {
  const factor : integer = 1
  const index : integer = 1

  while (index < a) {
    factor = factor * index
    index = index + 1

    if (index == a) {
      stdout("Fatorial atingido!")
    }
  }

  return factor
}

const test : integer = factorial(5)

stdout(test)
